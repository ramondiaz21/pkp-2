msgid "plugins.themes.default.name"
msgstr "Tema Padrão"

msgid "plugins.themes.default.description"
msgstr "Este tema implementa o tema padrão."

msgid "plugins.themes.default.option.typography.label"
msgstr "Tipografia"

msgid "plugins.themes.default.option.typography.description"
msgstr "Escolha uma combinação de fontes adequada a esta editora."

msgid "plugins.themes.default.option.typography.notoSans"
msgstr ""
"Noto Sans: Uma fonte nativa digital desenhada pelo Google para suporte "
"extensivo de linguagem."

msgid "plugins.themes.default.option.typography.notoSerif"
msgstr "Noto Serif: Uma variante serifada da fonte nativa digital do Google."

msgid "plugins.themes.default.option.typography.notoSerif_notoSans"
msgstr ""
"Noto Serif/Noto Sans: Uma combinação complementar com títulos serifados e "
"corpo do texto sem serifa."

msgid "plugins.themes.default.option.typography.notoSans_notoSerif"
msgstr ""
"Noto Sans/Noto Serif: Uma combinação complementar com títulos sem serifa e "
"corpo do texto serifado."

msgid "plugins.themes.default.option.typography.lato"
msgstr "Lato: Uma fonte popular sem serifa."

msgid "plugins.themes.default.option.typography.lora"
msgstr "Lora: Uma fonte serifada de largo espectro boa para leitura online."

msgid "plugins.themes.default.option.typography.lora_openSans"
msgstr ""
"Lora/Open Sans: Uma combinação complementar com títulos serifados e corpo do "
"texto sem serifa."

msgid "plugins.themes.default.option.colour.label"
msgstr "Cor"

msgid "plugins.themes.default.option.useHomepageImageAsHeader.option"
msgstr "Mostrar a imagem da página de início como plano de fundo do cabeçalho."

msgid "plugins.themes.default.option.showCatalogSeriesListing.label"
msgstr "Mostrar Séries"

msgid "plugins.themes.default.option.showCatalogSeriesListing.option"
msgstr ""
"Adicione uma lista de links para todas as séries da editora na página do "
"catálogo"

msgid "plugins.themes.default.option.displayStats.label"
msgstr "Opções de exibição de estatísticas de uso"

msgid "plugins.themes.default.option.displayStats.none"
msgstr "Não exibir o gráfico de estatísticas de uso das submissões ao leitor."

msgid "plugins.themes.default.option.displayStats.bar"
msgstr "Use um gráfico de barras para exibição das estatísticas de uso."

msgid "plugins.themes.default.displayStats.downloads"
msgstr "Downloads"

msgid "plugins.themes.default.displayStats.noStats"
msgstr "Os dados de downloads ainda não está disponíveis."

msgid "plugins.themes.default.option.colour.description"
msgstr "Escolha uma cor para o cabeçalho."

msgid "plugins.themes.default.option.useHomepageImageAsHeader.label"
msgstr "Imagem de Fundo do Cabeçalho"

msgid "plugins.themes.default.option.useHomepageImageAsHeader.description"
msgstr ""
"Quando uma imagem da página de início tiver sido carregada, mostrá-la como "
"fundo do cabeçalho em vez de ficar na sua posição habitual na página de "
"início."

msgid "plugins.themes.default.option.displayStats.line"
msgstr "Use um gráfico de linhas para exibição das estatísticas de uso."

msgid "plugins.themes.default.displayStats.monthInitials"
msgstr "Jan Fev Mar Abr Mai Jun Jul Ago Set Out Nov Dez"
